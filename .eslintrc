{
  "extends": ["prettier", "plugin:react/recommended"],
  "parser": "typescript-eslint-parser",
  "plugins": ["prettier", "typescript", "react"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  }
}
